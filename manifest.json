{
    "manifest_version": 3,
    "name": "Cookie_Lexic",
    "description": "Helps Dyslexic People",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "icons":{
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "128": "images/icon-128.png"
    },
    "background":{
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "scripting",
        "activeTab"
    ]
}
  
